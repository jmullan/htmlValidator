      </optional>
    </interleave>
  </define>
  <define name="aria.role.article">
    <attribute name="role">
      <value type="string">article</value>
    </attribute>
  </define>
  <define name="common.attrs.aria" combine="choice">
    <ref name="aria.article"/>
  </define>
  <define name="common.attrs.aria.landmark.article" combine="choice">
    <ref name="aria.article"/>
  </define>
